<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Backwards compatibility patch for RimWorld 1.2, which disables meal and ingredient blacklists introduced in Replimat v1.2.11 -->
	
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName="ReplimatComputer"]/comps/li[@Class="Replimat.CompProperties_ReplimatRestrictions"]
				</xpath>
			</li>
		</operations>
	</Operation>

</Patch>